


<div class="container-app">
    {{!-- <h2>Trainings</h2> --}}

    {{#each model as |training|}}
    
    <div class="trainig-container">
        <h3>{{training.title}}</h3>
        <p>{{training.description}}</p>
        <footer>
            <span>{{training.date}}</span>
            <div class="buttons-container">
            
                 <button  {{action 'editTraining' trainings}} class="btn edit blue">Edit</button>
                 <button  {{action 'deleteTraining' trainings}} class="btn edit red">Delete</button>
            </div>
        </footer>
        
    </div>
        
        
        
    
    {{/each}}
</div>